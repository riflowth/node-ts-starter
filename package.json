{
  "name": "node-ts-starter",
  "version": "1.0.0",
  "main": "dist/app.js",
  "author": "Vectier",
  "private": true,
  "scripts": {
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "build": "yarn build:prod",
    "dev": "concurrently -n runner,builder -c bgGreen,bgGray -i \"yarn --silent start:dev\" \"yarn --silent build:dev --watch\"",
    "predev": "yarn --silent build:dev --no-stats",
    "lint": "eslint . --ext .ts",
    "start:dev": "nodemon dist/app.js -q -w dist -e js",
    "start:prod": "node dist/app.js",
    "start": "yarn start:prod",
    "test": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "concurrently": "^7.1.0",
    "esbuild-loader": "^2.19.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.2.8",
    "jest": "^28.0.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": "16.15.0",
    "npm": "please-use-yarn"
  }
}
